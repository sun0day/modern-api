{
  "name": "wodash",
  "version": "0.1.0",
  "description": "Easy, tiny, modern web APIs lib",
  "author": "sun0day",
  "license": "ISC",
  "keywords": [
    "wodash",
    "web api",
    "utils"
  ],
  "main": "index.js",
  "scripts": {
    "build:all": "pnpm -r --filter=\"wodash.*\" run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prepublishOnly": "vitest",
    "test": "vitest",
    "test:cov": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@vitest/coverage-c8": "^0.24.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.25.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "^13.0.3",
    "semver": "^7.3.8",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vitest": "^0.24.0"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "eslint --fix"
    ]
  }
}
